<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<ScrollPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.shop.projet_shop.WishListController"
            prefHeight="400.0" prefWidth="600.0"
            pannable="true" fitToWidth="false">
    <content>
        <VBox fx:id="wishListWrapper"
              alignment="CENTER"
              spacing="15"
              prefWidth="600.0"
              style="-fx-padding: 30; -fx-alignment: center;">
            <VBox fx:id="wishListContainer"
                  alignment="CENTER"
                  spacing="15">
            </VBox>
        </VBox>
    </content>
</ScrollPane>
